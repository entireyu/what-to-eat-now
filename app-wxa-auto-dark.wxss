@media (prefers-color-scheme: dark) {
    page {
        background-color: rgb(24, 24, 26);
        color: rgb(238, 238, 241);
    }

    .yui-word-color-tip {
        color: rgb(116, 116, 121);
    }

    .yui-default-bg {
        background-color: #232838;
    }

    .yui-default-shadow {
        box-shadow: rgb(0, 0, 0);
