@media (prefers-color-scheme: dark) {
    .menu .item {
        background-color: rgb(18, 30, 38);
        border: rgb(0, 0, 0);
        color: rgb(0, 102, 176);
    }

    .userinput {
        border: rgb(0, 0, 0);
    }

    .btnGroup button[type='primary'] {
        background-color: rgb(0, 102, 176);
    }

    .btnGroup button[type='default'] {
        color: rgb(238, 238, 241);
    }
}
